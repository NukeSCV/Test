<script>
var x = prompt("Введите ваше любимое число!")
switch (x) {
  case '1':
    alert( 'Вы умрете через ' + x + 'год' );
    break;
  case '2':
  case '3':
  case '4':
    alert( 'Вы умрете через ' + x + 'года' );
    break;
  case 5:
  case 6:
    alert( 'Вы умрете через ' + x + 'лет' );
    break;
    
  default:
    alert('Будете страдать: ' + x 'лет')
}
</script>
